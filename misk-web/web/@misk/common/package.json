{
  "name": "@misk/common",
  "version": "0.1.0-alpha-0.2",
  "license": "SEE LICENSE IN https://github.com/square/misk-web",
  "description": "Microservice Kontainer Common Libraries, Externals, Styles",
  "author": "Square/Misk Authors (https://github.com/square/misk-web/graphs/contributors)",
  "main": "lib/web/@misk/common/common.js",
  "module": "lib/web/@misk/common/common.js",
  "typings": "lib/web/@misk/common/index.d.ts",
  "style": "lib/web/@misk/common/styles.js",
  "engines": {
    "node": ">=7.8.0"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:square/misk-web.git"
  },
  "scripts": {
    "build": "yarn run lint && yarn run lib",
    "clean": "rm -rf cachedUrls lib",
    "dev": "yarn run build; yarn run updateDevCache",
    "ci-build": "yarn run clean && yarn run lib",
    "lib": "yarn run refresh; webpack --config webpack.config.js && webpack --config webpack.config.static.js && webpack --config webpack.config.vendors.js",
    "lint": "prettier --write --config prettier.config.js \".{/src/**/,/}*.{md,css,sass,less,json,js,jsx,ts,tsx}\"",
    "prepare": "cd ../../../../ && ./gradlew web -Pcmd='-a' -Ptabs='@misk/common'",
    "refresh": "node refreshCachedUrls.js",
    "reinstall": "rm -rf node_modules && yarn install",
    "updateDevCache": "cd ..; ./updateLocalCache.sh",
    "test": "echo no tests"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.8.0",
    "@blueprintjs/datetime": "^3.3.1",
    "@blueprintjs/icons": "^3.3.0",
    "@blueprintjs/select": "^3.2.1",
    "@blueprintjs/table": "^3.3.0",
    "@blueprintjs/timezone": "^3.1.0",
    "axios": "^0.18.0",
    "connected-react-router": "^5.0.1",
    "dayjs": "^1.7.7",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.12",
    "lodash-es": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.12",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "skeleton-css": "^2.0.4",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@misk/dev": "^0.1.0-alpha-0.1",
    "@misk/tslint": "^0.1.0-alpha-0.1"
  },
  "miskCachedUrls": {
    "sq-market-font": {
      "filepath": "sq-market.css",
